const appBootstraper=require("../src/app");
const http = require("http");

appBootstraper().then(app=>{
    const port = process.env.PORT || 3000;
    app.set("port",port);

    const server = http.createServer(app);
    server.listen(port);
    console.log(`Application started on port ${port}`);
})
.catch(console.log);